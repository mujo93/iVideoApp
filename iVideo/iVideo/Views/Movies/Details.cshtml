@model iVideo.Models.Movie
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<li>Genre: @Model.Genre.Name</li>
<li>Release Date: @Model.ReleaseDate.Day,@Model.ReleaseDate.Month,@Model.ReleaseDate.Date,@Model.ReleaseDate.Year</li>
<li>Date Added: @Model.DateAdded</li>
<li>Number in Stock: @Model.NumberInStock</li>



